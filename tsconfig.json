{
    "compilerOptions": {
      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "alwaysStrict": true,
      "charset": "UTF-8",
      "diagnostics": true,
      "moduleResolution": "node",
      "noErrorTruncation": false,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "outDir": "./out/",
      "pretty": true,
      "declaration": false,
      "removeComments": false,
      "strict": true,
      "strictFunctionTypes": true,
      "target": "es2019", // 3 years ago JavaScript is up to date enough, right???
      "watch": false
    },
      "files": [
          "./src/index.ts"
      ],
      "types": [
      ],
      "typeRoots": [
          "node_modules/@types"
      ],
      "lib": [
          "dom",
          "es2019"
      ]
  }